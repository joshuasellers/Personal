  {
    "name": "YouTube Use Tracker",
    "version": "1.0",
    "description": "Tracks time spent on YouTube to improve productivity.",
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": ["activeTab", "browsingData", "cookies", "declarativeContent", "history", "webRequestBlocking"],
    "manifest_version": 2
  }