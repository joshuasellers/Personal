  {
    "name": "YouTube Use Tracker",
    "version": "3.0",
    "description": "Tracks time spent on YouTube to improve productivity.",
    "background": {
      "scripts": ["format.js", "background.js"],
      "persistent": false
     },
    "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "webNavigation", "alarms"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
     "content_scripts": [
    {
      "matches": ["http://*.youtube.com/*"],
      "all_frames": true,
      "js": ["contentScript.js"]
    }
  ],
    "manifest_version": 2
  }